<ol id="quips">
  <%= render :partial => 'quips' %>
</ol>

<% content_for :javascript do %>
  <%= javascript_tag "var ep = new EndlessPage(#{@quips.respond_to?(:total_pages) ? @quips.total_pages : 1}, $('quips'), '#{quips_path(:format => :js, :q => params[:q])}', '#{form_authenticity_token}');" %>
<% end %>
